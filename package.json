  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev:web & npm run dev:admin",
    "dev:web": "VITE_ROOT=./web vite --config ./web/vite.config.ts",
    "dev:admin": "VITE_ROOT=./admin vite --config ./admin/vite.config.ts",
    "type": "vue-tsc -b",
    "build:web": "vite build --config ./web/vite.config.ts",
    "build:admin": "vite build --config ./admin/vite.config.ts",
    "build": "npm run type & npm run build:web & npm run build:admin",
    "preview": "vite preview --config ./web/vite.config.ts"
  },
